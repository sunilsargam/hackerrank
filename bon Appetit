#include<iostream>
using namespace std;
int main()
{
    int k,n,sum=0,b;
    cin>>n>>k;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        sum+=a[i];
    
    }
    cin>>b;
    int temp=0;
    temp=(sum-a[k])/2;
    if(b-temp==0)
    cout<<"Bon Appetit";
    else
    cout<<b-temp;
}
